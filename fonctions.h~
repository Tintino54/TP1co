#ifndef FONCTIONS_H
#define FONCTIONS_H

#include <stdio.h>
#include <stdlib.h>

	/* Créations des structures */

struct node {
	int x_node;
	int y_node;
	int id_node;
};
typedef struct node node;


struct edge {
	int node_a;
	int node_b;
	double cost;
};
typedef struct edge edge;

	/* Créations des fonctions */



#endif
